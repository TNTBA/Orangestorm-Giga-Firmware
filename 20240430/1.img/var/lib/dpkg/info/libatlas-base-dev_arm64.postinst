#!/bin/sh

set -e

update-alternatives --install /usr/lib/aarch64-linux-gnu/libblas.so libblas.so-aarch64-linux-gnu \
                    /usr/lib/aarch64-linux-gnu/atlas/libblas.so 35 \
                    --slave /usr/lib/aarch64-linux-gnu/libblas.a libblas.a-aarch64-linux-gnu \
                    /usr/lib/aarch64-linux-gnu/atlas/libblas.a \
                    --slave /usr/include/aarch64-linux-gnu/cblas.h cblas.h-aarch64-linux-gnu \
                    /usr/include/aarch64-linux-gnu/cblas-atlas.h \
                    --slave /usr/lib/aarch64-linux-gnu/pkgconfig/blas.pc \
                    blas.pc-aarch64-linux-gnu /usr/lib/aarch64-linux-gnu/pkgconfig/blas-atlas.pc

update-alternatives --install /usr/lib/aarch64-linux-gnu/liblapack.so \
                    liblapack.so-aarch64-linux-gnu \
                    /usr/lib/aarch64-linux-gnu/atlas/liblapack.so 35 \
                    --slave /usr/lib/aarch64-linux-gnu/liblapack.a liblapack.a-aarch64-linux-gnu \
                    /usr/lib/aarch64-linux-gnu/atlas/liblapack.a \
                    --slave /usr/lib/aarch64-linux-gnu/pkgconfig/lapack.pc \
                    lapack.pc-aarch64-linux-gnu \
                    /usr/lib/aarch64-linux-gnu/pkgconfig/lapack-atlas.pc



exit 0
